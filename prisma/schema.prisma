datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL") // Replace with your Vercel Postgres connection URL
}

generator client {
  provider = "typescript"
}

model Booking {
  id        Int      @id @default(autoincrement())
  subject   String
  startTime DateTime
  endTime   DateTime
  // Add other booking fields as needed
}
